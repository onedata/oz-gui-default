<a {{action 'clicked'}}
  class="login-icon-box v-center-parent {{type}}">
  {{#if active}}
    <div class="v-center-child">
      {{spin-spinner scale=0.4 lines=12 length=20 width=10}}
    </div>
  {{else}}
    {{#if (eq iconType "oneicon")}}
      {{one-icon tagName="div" icon=iconName class="v-center-child"}}
    {{else}}
      <div class="social-icon-image" style={{socialIconStyle}}></div>
    {{/if}}
  {{/if}}
</a>
